#include<bits/stdc++.h>
using namespace std;
int main()
{
    int j,i,n,a,cnt;
    for(j=1;scanf("%d",&n);j++)
    {
        if(n==0) break;
        cnt=0;
        for(i=0;i<n;i++)
        {
            cin>>a;
            if(a>0) cnt++;
            else cnt--;
        }
        printf("Case %d: %d\n",j,cnt);
    }
return 0;
}
