#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    int n,cnt1,cnt2;
    cin>>n;
    while(n--)
    {
        cin>>s;
        cnt1=0;
        cnt2=0;
        if(s.size()==5) cout<<"3"<<endl;
        else
        {
           if(s[0]=='o')cnt1++;
           else if(s[0]=='t') cnt2++;
           if(s[1]=='n') cnt1++;
           else if(s[1]=='w') cnt2++;
           if(s[2]=='e') cnt1++;
           else if(s[2]=='o') cnt2++;
           if(cnt1>cnt2) cout<<"1"<<endl;
           else cout<<"2"<<endl;
        }

    }
return 0;
}
