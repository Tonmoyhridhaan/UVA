#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    int t=0;
    while(getline(cin,s))
    {
        t++;
        if (s=="*") break;
        else if(s=="Hajj") cout<<"Case "<<t <<": "<<"Hajj-e-Akbar"<<"\n";
        else if(s=="Umrah") cout<<"Case "<<t <<": "<<"Hajj-e-Asghar"<<"\n";
    }
    return 0;
}
