#include<bits/stdc++.h>
#define sf scanf
#define pf printf
#define i64 long long int
using namespace std;
int main()
{
    int n;
    while(sf("%d",&n)==1)
    {
        int cnt = 1;
        i64 val = 1;

        while(val!=0)
        {
            val = ((val*10) +1)%n;
            cnt++;
        }

        pf("%d\n",cnt);
    }

return 0;

}
