#include<stdio.h>
main()
{
    int t,a,b,i,sum;
    scanf("%d",&t);
    if(t<=100){
    for(i=1;i<=t;i++)
    {
        scanf("%d",&a);
        scanf("%d",&b);
        sum=0;
        while(a<=b)
        {
            if(a%2!=0) sum=sum+a;
            a++;
        }
        printf("Case %d: %d\n",i,sum);
    }

    }

return 0;
}
