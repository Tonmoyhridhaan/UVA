#include<stdio.h>
#include<stdlib.h>
main()
{
    int t;
    long long int s,d,a,b,i,j;
    scanf("%d",&t);
    while(t--)
    {
        scanf("%lld%lld",&s,&d);
        a=abs((s/2)+(d/2)+(s%2));
        b=abs((s/2)-(d/2));
        if((a-b)==d) printf("%lld %lld\n",a,b);
        else printf("impossible\n");
    }
    return 0;


}
