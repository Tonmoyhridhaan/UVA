#include<stdio.h>
main()
{
    int t, a,b,c, i,j;
    scanf("%d", &t);
    for(i=1;i<=t;i++)
    {
        scanf("%d%d%d",&a,&b,&c);
        if((a>b && a<c) || (a>c && a<b)) printf("Case %d: %d",i,a);
        else if((b>a && b<c) || (b<a && b>c)) printf("Case %d: %d",i,b);
        else if((c>a && c<b) || (c<a && c>b)) printf("Case %d: %d",i,c);
        printf("\n");

    }
    return 0;
}
