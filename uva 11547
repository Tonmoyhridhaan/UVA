#include<stdio.h>
#include<stdlib.h>
main()
{
    long long int n;
    int t;
    scanf("%d",&t);
    while(t--)
    {
        scanf("%lld",&n);
        n*=567;
        n/=9;
        n+=7492;
        n*=235;
        n/=47;
        n-=498;
        n%=100;
        n/=10;
        n=abs(n);
        printf("%lld\n",n);
    }


return 0;
}
