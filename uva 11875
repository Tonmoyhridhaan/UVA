#include<bits/stdc++.h>
using namespace std;
#define sf scanf
#define pf printf
#define pb push_back
#define all(a) a.begin(),a.end()
#define i64 long long int
const double pi = 2*acos(0) ;
const int maxn = (i64)2e5+5;

int main()
{
    int n,a,i,j;
    cin>>n;
    int arr[n];
    for(j = 1 ; j<=n ; j++)
    {
        cin>>a;
        for(i = 0 ; i<a ; i++)
        {
            cin>>arr[i];
        }
        sort(arr,arr+a);
        pf("Case %d: %d\n",j,arr[a/2]);
    }

return 0;
}
